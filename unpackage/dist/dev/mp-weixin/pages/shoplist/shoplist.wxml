<view class="shoplistContainer"><view class="search"><view class="searchInp"><image class="searchImg" src="../../static/images/search.png" mode></image><input class="inp" type="text" value="" placeholder="点击搜索商品"/></view></view><view class="header"><block wx:for="{{navList}}" wx:for-item="nav" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['sort',[index]]]]]}}" class="{{['item',(currentIndex===index)?'active':'']}}" bindtap="__e">{{nav}}</view></block></view><view class="shoplist"><block wx:for="{{shopList2}}" wx:for-item="item" wx:for-index="__i0__" wx:key="gid"><view class="shopItem"><image class="shopImg" src="{{item.photo}}" data-event-opts="{{[['tap',[['toDetail',['$0'],[[['shopList2','gid',item.gid]]]]]]]}}" bindtap="__e"></image><view class="detail"><view data-event-opts="{{[['tap',[['toDetail',['$0'],[[['shopList2','gid',item.gid]]]]]]]}}" class="title" bindtap="__e">{{item.subject}}</view><view class="price"><text class="priceText">{{"￥"+item.sale_price}}</text><text class="dprice">{{item.dprice}}</text></view><view class="sold"><text class="comments">{{item.comments}}</text><text class="soldNum">{{item.sold}}</text><image class="toCart" src="../../static/images/addcart.png" data-event-opts="{{[['tap',[['toCart',['$0'],[[['shopList2','gid',item.gid]]]]]]]}}" bindtap="__e"></image></view></view></view></block></view></view>