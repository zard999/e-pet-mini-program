.loginContainer {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.loginContainer .logo {
  width: 300rpx;
  height: 300rpx;
}
.loginContainer .title {
  font-size: 24rpx;
  font-weight: 100;
}
.loginContainer .iphone-login {
  width: 90%;
  height: 100rpx;
  color: #fff;
  background-color: #74be5e;
  line-height: 100rpx;
  text-align: center;
  border-radius: 50rpx;
  font-size: 32rpx;
  margin: 30rpx 0;
}
.loginContainer .other-phone {
  font-size: 24rpx;
  color: #999;
}

