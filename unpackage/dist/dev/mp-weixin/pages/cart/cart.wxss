.cartContainer {
  height: 100%;
}
.cartContainer .cartScroll {
  height: calc(100vh - 138rpx);
  background-color: #f3f4f5;
}
.cartContainer .cartScroll .cartList {
  width: 100%;
  padding: 0 20rpx 0 20rpx;
  background-color: #fff;
}
.cartContainer .cartScroll .cartList .cartItem {
  display: flex;
  align-items: center;
  padding: 30rpx;
}
.cartContainer .cartScroll .cartList .cartItem .checkbox .noCheckImg {
  width: 40rpx;
  height: 40rpx;
}
.cartContainer .cartScroll .cartList .cartItem .checkbox .noCheckImg.selected {
  background: url(data:image/png;base64,);
  background-size: cover;
}
.cartContainer .cartScroll .cartList .cartItem .itemContent {
  display: flex;
  margin-left: 20rpx;
  width: 100%;
}
.cartContainer .cartScroll .cartList .cartItem .itemContent .itemImg {
  width: 180rpx;
  height: 180rpx;
  flex-shrink: 0;
}
.cartContainer .cartScroll .cartList .cartItem .itemContent .rightContent {
  width: 100%;
  display: flex;
  flex-direction: column;
  margin-left: 20rpx;
}
.cartContainer .cartScroll .cartList .cartItem .itemContent .rightContent .title {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2 /* * 多行文本溢出... */;
  overflow: hidden;
  font-size: 26rpx;
}
.cartContainer .cartScroll .cartList .cartItem .itemContent .rightContent .price_and_addplus {
  display: flex;
  justify-content: space-between;
  margin-top: 64rpx;
  padding-right: 50rpx;
}
.cartContainer .cartScroll .cartList .cartItem .itemContent .rightContent .price_and_addplus .price {
  color: #ff5757;
  font-weight: 600;
}
.cartContainer .cartScroll .cartList .cartItem .itemContent .rightContent .price_and_addplus .add_plus {
  display: flex;
  align-items: center;
  box-sizing: border-box;
  text-align: center;
  border: 1rpx solid #999;
  height: 42rpx;
  border-radius: 20rpx;
}
.cartContainer .cartScroll .cartList .cartItem .itemContent .rightContent .price_and_addplus .add_plus .plus {
  width: 60rpx;
  border-right: #999 solid 1rpx;
  color: #999;
}
.cartContainer .cartScroll .cartList .cartItem .itemContent .rightContent .price_and_addplus .add_plus .plus.active {
  color: #000;
}
.cartContainer .cartScroll .cartList .cartItem .itemContent .rightContent .price_and_addplus .add_plus .inp {
  width: 60rpx;
  font-size: 24rpx;
  outline: none;
}
.cartContainer .cartScroll .cartList .cartItem .itemContent .rightContent .price_and_addplus .add_plus .add {
  width: 60rpx;
  height: 42rpx;
  border-left: #999 solid 1rpx;
}
.cartContainer .cartScroll .cartList .slot-button {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 120rpx;
  height: 100%;
  background-color: #e64340;
  color: #fff;
}
.cartContainer .settlement {
  position: absolute;
  bottom: 0;
  left: 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  background-color: #fff;
  padding: 30rpx;
  font-size: 26rpx;
  color: #999;
  z-index: 999;
}
.cartContainer .settlement .total {
  width: 40rpx;
  height: 40rpx;
}
.cartContainer .settlement .total.selected {
  background: url(data:image/png;base64,);
  background-size: cover;
}
.cartContainer .settlement .price {
  font-size: 34rpx;
  color: #ff5757;
  font-weight: 600;
}
.cartContainer .settlement .discount {
  font-size: 24rpx;
  color: #ff5757;
}
.cartContainer .settlement .toSettlement {
  color: #ff5757;
  font-size: 22rpx;
  margin-top: 8rpx;
  width: 180rpx;
  height: 70rpx;
  border-radius: 70px;
  font-size: 28rpx;
  line-height: 70rpx;
  text-align: center;
  color: #fff;
  font-size: 500;
  background-color: #41c856;
  margin-right: 50rpx;
}
.cartContainer .emptyCart {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.cartContainer .emptyCart .anyTo {
  width: 200rpx;
  height: 80rpx;
  color: #f13e3e;
  border: 2rpx solid #f13e3e;
  border-radius: 80rpx;
  box-sizing: border-box;
  text-align: center;
  line-height: 80rpx;
}

